<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.gapps.myfm.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_tactics"
        android:name="com.example.tactics_center.ui.TacticsFragment"
        android:label="@string/title_tactics"
        tools:layout="@layout/fragment_tactics" />

    <fragment
        android:id="@+id/navigation_search"
        android:name="com.gapps.myfm.search.SearchFragment"
        android:label="@string/title_search"
        tools:layout="@layout/fragment_search" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.gapps.player_center.ui.TeamPlayersFragment"
        android:label="@string/title_settings"
        tools:layout="@layout/fragment_team_players" />
</navigation>
